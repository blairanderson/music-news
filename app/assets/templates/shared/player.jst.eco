<div id="player">
  <ul class="player">
    <li><a id="previous"></a></li>
    <li><a id="nolove"></a></li>
    <li><a id="play" data-action="play-pause"></a></li>
    <li><a id="next"></a></li>
    <li><a target="_blank" class="current-song title" href="<%= @currentTrack.get('source_url')%>"><%= @currentTrack.get('title') %><i class="cloud"></i></a></li>
  </ul>
</div>
