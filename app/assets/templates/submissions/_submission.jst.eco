<% if @submission.songs.length: %>
    <div class="sub-content">
    </div>
    <div class="sub-gutter">
      <a href="/<%= @submission.get('id')%>">
        <span><%= @submission.get('title')%></span>
        <h6><%= @submission.get('fromNow')%> on <%= @submission.get('calendar')%></h6>
        <div class="img">
          <img src="<%= @submission.thumbnail_url %>" class="img-responsive">
        </div>
        <p class="lead"><%= @submission.get('body')%></p>
      </a>
    </div>
<% end %>
