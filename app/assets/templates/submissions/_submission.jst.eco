<% if @submission.songs.length: %>
  <div class="sub-gutter">
    <a href="<%= @submission.get('id')%>">
      <span><%= @submission.get('title')%></span>
      <div>submitted <%= @submission.fromNow() %></div>
      <div>
        <hr>
        <p><%= @submission.get('body') %></p>
      </div>
    </a>
  </div>
  <div class="sub-content">
  </div>
<% end %>
