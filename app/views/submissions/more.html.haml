%h4.text-center
  Latest Submissions to 
  =link_to "SEAinHD.com", seainhd_path
- @submissions.each do |submission|
  %p.lead
    = link_to submission.title, submission
  %p
    = submission.body
  %ul.unstyled
  - submission.soundclouds.each do |song|
    %li
      = "soundcloud: #{song.url}"
  %hr
